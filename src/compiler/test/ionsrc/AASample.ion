
// constant
false = 0
true = 1
//  type
Boolean = true | false
//  meta
@metaProperty = true

//  class
class Vector
    //  instance
    x: Number
    y: Number
    [@metaProperty]
    z: Number = 0
    //  meta property
    [@metaProperty] = true
    //  static function
    translate =
        ()
            //  parameter
            x: Number = 0
            [@metaProperty] = false
            y: Number = 0
        ->
            Vector(this.x + x, this.y + y)
    //  static function
    length = () -> (x * x + y * y) ** 0.5
//  final exported value
()
    Boolean
    Vector
    constant = 10
    BetweenTwoAndFive =
        &
            >= 2
            <= 5
