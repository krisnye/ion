
ssafunc(x: Float) =>
    total: Float = 0.0
    if x < 0.0
        total += 10.0
        total += 15.0
    else if x > 0.0
        total += 20.0
    else
        total += 30.0
    total += 5.0
    total += 1.0
    return total

