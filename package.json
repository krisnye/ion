{
    "name": "ion2",
    "version": "1.9.0",
    "main": "lib/index",
    "repository": "https://github.com/krisnye/ion.git",
    "bin": "lib/command.js",
    "scripts": {
        "dev": "npm link pegs && yarn install && mkdir -p lib",
        "build": "pegs src/parser.pegs lib/parser.js && tsc -p .",
        "watchBuild": "nodemon -w src/parser.pegs -x pegs src/parser.pegs lib/parser.js & tsc -p . --watch",
        "watchTest": "nodemon -w lib -w test -e js,ion lib/command.js test",
        "watchDebug": "nodemon -w lib -w test -e js,ion --inspect-brk lib/command.js test",
        "watch": "yarn watchBuild & yarn watchTest",
        "debug": "yarn watchBuild & yarn watchDebug"
    },
    "author": {
        "name": "Kris Nye",
        "email": "krisnye@gmail.com"
    },
    "contributors": [
        {
            "name": "Kody King"
        }
    ],
    "bugs": {
        "url": "https://github.com/krisnye/ionic/issues"
    },
    "homepage": "https://github.com/krisnye/ion",
    "dependencies": {
        "text-diff": "^1.0.1"
    },
    "devDependencies": {
        "@types/escodegen": "^0.0.6",
        "@types/node": "^7.0.22",
        "escodegen": "^1.8.1",
        "jsondiffpatch": "^0.2.4",
        "pegs": "*"
    }
}
